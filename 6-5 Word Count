#include <string.h>
#include <stdio.h>

#define MAX 100
typedef struct tagWordCounter
{
    char word[20];
    int count;
}WordCounter;

WordCounter wc[MAX];
int num=0;

void sort(struct tagWordCounter *data,int num);

int main()
{
    
    char wd[20];
    int i;
    char ch;
begin:
    
    ///////////////
    //// RESET/////
    ///////////////
    
    for (int k=0; k<MAX; k++) {
        memset(&(wc[k].count), 0, sizeof(wc[k].count));
        strcpy(wc[k].word,"");
    }
    num=0;
    ///////////////
    //// RESET/////
    ///////////////
    
    while(1)
    {
        if(scanf("%s",wd)==EOF)
        {
            return 0;
        }
        for(i=0;i<num;i++)
            if(strcmp(wd,wc[i].word)==0)
            {
                wc[i].count++;
                break;
            }
        if(i==num)
        {
            num++;
            strcpy(wc[num-1].word,wd);
            wc[num-1].count=1;
        }
        ch=getc(stdin);
        if(ch==10 || ch==13)
            break;
    }
    sort(wc, num);
    
    ///////////////////
    ////PRINT OUTPUT///
    ///////////////////
    
    for(i=0;i<num;i++)
    {
        printf("%s %d\n",wc[i].word,wc[i].count);
    }
    goto begin;
}

void sort(struct tagWordCounter *data,int num)
{
    char temp[20];
    int temp2;
    for (int j=num-1; j>0; j--)
    {
        for (int k=0; k<=j-1; k++)
        {
            if (strcmp(data[k].word,data[k+1].word)>0)
            {
                strcpy(temp, data[k].word);
                strcpy(data[k].word, data[k+1].word);
                strcpy(data[k+1].word, temp);
            
                temp2=data[k].count;
                data[k].count=data[k+1].count;
                data[k+1].count=temp2;
            
            }
        }
    }
}
