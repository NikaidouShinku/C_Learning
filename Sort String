#include <string.h>
#include <stdio.h>
void sort(char (*stack)[40],int i);
int main(int argc, const char * argv[])
{
    char stack[100][40];
    int i=0;

    while (1)
    {
        if(gets(stack[i]))
        {
            i++;
        }
        else
        {
            sort(stack,i);
            for (int j=0; j<i; j++)
            {
                printf("%s\n",stack[j]);
            }
            return 0;
        }
    }
}

void sort(char (*data)[40],int i)
{
    char temp[40];
    for (int j=i-1; j>0; j--)
    {
        for (int k=0; k<=j-1; k++)
        {
            if (data[j][0] > data[j + 1][0])
            {
                strcpy(temp, data[k]);
                strcpy(data[k], data[k+1]);
                strcpy(data[k+1], temp);
            }
            else if(data[j][0] == data[j + 1][0])
            {
                int s=1;
                while (data[j][s] == data[j + 1][s])
                {
                    s++;
                }
                if (data[j][s] > data[j + 1][s])
                {
                strcpy(temp, data[k]);
                strcpy(data[k], data[k+1]);
                strcpy(data[k+1], temp);
                }
            }
        }
    }
}


