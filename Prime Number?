#include <stdio.h>
int is_prime(int n);
int main()
{
    int test;
    while (scanf("%d",&test)!=EOF) {
        if (is_prime(test)) {
            printf("yes\n");
        }
        else
            printf("no\n");
    }
}
int is_prime(int n)
{
    for (int i=2; i<=(n/2); i++) {
        if (n%i==0) {
            return 0;
        }
    }
  
